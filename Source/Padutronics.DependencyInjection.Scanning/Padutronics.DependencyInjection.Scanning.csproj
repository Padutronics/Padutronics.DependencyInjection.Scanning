<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <TargetFramework>net7.0</TargetFramework>
        <LangVersion>latest</LangVersion>
        <Nullable>enable</Nullable>
        <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
        <WarningLevel>5</WarningLevel>
        <Version>0.0.0</Version>
        <Authors>Pavel Dubsky</Authors>
        <Company>Padutronics</Company>
        <Product>Padutronics Framework</Product>
        <Description>Dependency injection scanning utilities.</Description>
        <Copyright>Copyright Â© Padutronics 2022</Copyright>
        <PackageProjectUrl>https://github.com/Padutronics/Padutronics.DependencyInjection.Scanning</PackageProjectUrl>
        <RepositoryUrl>https://github.com/Padutronics/Padutronics.DependencyInjection.Scanning</RepositoryUrl>
        <RepositoryType>Git</RepositoryType>
    </PropertyGroup>
    <ItemGroup>
        <PackageReference Include="Padutronics.DependencyInjection" Version="0.1.0" />
        <PackageReference Include="Padutronics.Reflection" Version="0.3.0" />
    </ItemGroup>
</Project>